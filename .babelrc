{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-typescript",
        "@babel/preset-react"
    ],
    "plugins": [
        "@babel/proposal-class-properties",
        "@babel/proposal-object-rest-spread",
        ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": true }] // `style: true` for less
    ]
}
